{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node",
        "install": {
            "include": ["package.json"],
            "additionalSteps": ["npm i --legacy-peer-deps"]
          } 
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/" 
      }
    
    ]
  
  }
  